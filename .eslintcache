nt = meals.map((meal) => (\n        <MealItem\n          key={meal.id}\n          id={meal.id}\n          name={meal.name}\n          description={meal.description}\n          price={meal.price}\n        />\n      ));\n    }\n  }\n\n  if (error) {\n    content = (\n      <div className=\"centered\">\n        <h2>{content}</h2>\n      </div>\n    );\n  }\n\n  return (\n    <section className={classes.meals}>\n      <Card>\n        <ul>{content}</ul>\n      </Card>\n    </section>\n  );\n};\n\nexport default AvailableMeals;\n","/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/components/Layout/HeaderCartButton.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/components/Cart/CartItem.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/components/UI/Modal.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/components/UI/Card.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/components/Cart/CartIcon.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/components/Meals/MealItem/MealItem.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/components/Meals/MealItem/MealItemForm.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/components/UI/Input.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/hooks/use-http.js",["104"],"import { useCallback, useReducer, useRef } from \"react\";\n\nconst httpReducer = (state, action) => {\n  if (action.type === \"SEND\") {\n    return {\n      data: null,\n      error: null,\n      status: \"pending\",\n    };\n  }\n\n  if (action.type === \"SUCCESS\") {\n    return {\n      data: action.responseData,\n      error: null,\n      status: \"success\",\n    };\n  }\n\n  if (action.type === \"ERROR\") {\n    return {\n      data: null,\n      error: action.errorMessage,\n      status: \"failed\",\n    };\n  }\n  return state;\n};\n\nconst useHttp = (requestFn, startWithPending) => {\n  const [httpState, dispatch] = useReducer(httpReducer, {\n    status: startWithPending ? \"pending\" : null,\n    data: null,\n    error: null,\n  });\n\n  const sendRequest = useCallback(\n    async (requestData) => {\n      dispatch({ type: \"SEND\" });\n      try {\n        const responseData = await requestFn(requestData);\n        dispatch({\n          type: \"SUCCESS\",\n          responseData,\n        });\n      } catch (error) {\n        dispatch({\n          type: \"ERROR\",\n          errorMessage: error.message || \"Some thing went wrong!\",\n        });\n      }\n    },\n    [requestFn]\n  );\n\n  return {\n    sendRequest,\n    ...httpState,\n  };\n};\n\nexport default useHttp;\n","/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/components/UI/LoadingSpinner.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/lib/api.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/lib/coreApi.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/components/Cart/Checkout.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/utils/index.js",[],"/media/lengoaingu/PROJECT/Reactjs-Udemy/food-order-app-with-form-and-http/src/utils/validate.js",[],{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","severity":1,"message":"110","line":4,"column":21,"nodeType":"111","messageId":"112","endLine":4,"endColumn":29},{"ruleId":"109","severity":1,"message":"113","line":1,"column":35,"nodeType":"111","messageId":"112","endLine":1,"endColumn":41},"no-native-reassign",["114"],"no-negated-in-lhs",["115"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'useRef' is defined but never used.","no-global-assign","no-u